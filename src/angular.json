{
  "version": 1,
  "projects": {
    "express-backend": {
      "root": "apps/express-backend",
      "sourceRoot": "apps/express-backend/src",
      "projectType": "application",
      "prefix": "express-backend",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/express-backend",
            "main": "apps/express-backend/src/main.ts",
            "tsConfig": "apps/express-backend/tsconfig.app.json",
            "assets": ["apps/express-backend/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/express-backend/src/environments/environment.ts",
                  "with": "apps/express-backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "express-backend:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/express-backend/tsconfig.app.json",
              "apps/express-backend/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/express-backend/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/express-backend/jest.config.js",
            "tsConfig": "apps/express-backend/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "interfaces": {
      "root": "libs/interfaces",
      "sourceRoot": "libs/interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/interfaces/tsconfig.lib.json",
              "libs/interfaces/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/interfaces/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/interfaces/jest.config.js",
            "tsConfig": "libs/interfaces/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "angular-web-client": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/angular-web-client",
      "sourceRoot": "apps/angular-web-client/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular-web-client",
            "index": "apps/angular-web-client/src/index.html",
            "main": "apps/angular-web-client/src/main.ts",
            "polyfills": "apps/angular-web-client/src/polyfills.ts",
            "tsConfig": "apps/angular-web-client/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/angular-web-client/src/favicon.ico",
              "apps/angular-web-client/src/assets"
            ],
            "styles": ["apps/angular-web-client/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular-web-client/src/environments/environment.ts",
                  "with": "apps/angular-web-client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-web-client:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-web-client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-web-client:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular-web-client/tsconfig.app.json",
              "apps/angular-web-client/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/angular-web-client/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/angular-web-client/jest.config.js",
            "tsConfig": "apps/angular-web-client/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/angular-web-client/src/test-setup.ts"
          }
        }
      }
    },
    "angular-web-client-e2e": {
      "root": "apps/angular-web-client-e2e",
      "sourceRoot": "apps/angular-web-client-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular-web-client-e2e/cypress.json",
            "tsConfig": "apps/angular-web-client-e2e/tsconfig.e2e.json",
            "devServerTarget": "angular-web-client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-web-client:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/angular-web-client-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/angular-web-client-e2e/**"]
          }
        }
      }
    },
    "core-data": {
      "projectType": "library",
      "root": "libs/core-data",
      "sourceRoot": "libs/core-data/src",
      "prefix": "workspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/core-data/tsconfig.lib.json",
              "libs/core-data/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core-data/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core-data/jest.config.js",
            "tsConfig": "libs/core-data/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/core-data/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "auth": {
      "projectType": "library",
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "prefix": "workspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/auth/tsconfig.lib.json",
              "libs/auth/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/auth/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/auth/jest.config.js",
            "tsConfig": "libs/auth/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/auth/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "common": {
      "projectType": "library",
      "root": "libs/common",
      "sourceRoot": "libs/common/src",
      "prefix": "workspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/common/tsconfig.lib.json",
              "libs/common/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/common/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/common/jest.config.js",
            "tsConfig": "libs/common/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/common/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "express-backend"
}
